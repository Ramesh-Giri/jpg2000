{
    "version": 2,
    "builds": [
      {
        "src": "api/convertAndSaveImage.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/convertAndSaveImage.js"
      }
    ],
    "build": {
      "env": {
        "OPENJPEG_VERSION": "v2.5.0"
      }
    }
  }
  