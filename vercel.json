{
    "version": 2,
    "builds": [
      {
        "src": "api/convertAndSaveImage.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "openjpeg/bin/opj_decompress"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/convertAndSaveImage.js"
      }
    ]
  }
  