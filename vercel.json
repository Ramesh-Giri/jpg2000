{
    "version": 2,
    "builds": [
      {
        "src": "api/convertAndSaveImage.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/convertAndSaveImage.js"
      }
    ],
    "functions": {
      "api/convertAndSaveImage.js": {
        "includeFiles": "openjpeg/**"
      }
    }
  }
  